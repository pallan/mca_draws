    //    e.attr({rx: 5, ry: 3}).animateAlong(p, 4000, true, function () {
    //      e.attr({rx: 4, ry: 4});
    //    });
    var e = paper.circle(900, 120, 25).attr({stroke: "#999", fill: "#f00"});
    //e.animate({cx: 204, cy: 92}, 4000, ">");
    e.animate({
        "20%": {cx: 700, cy: 120},
        "40%": {cx: 500, cy: 114},
        "60%": {cx: 350, cy: 106},
        "80%": {cx: 250, cy: 96},
        "100%": {cx: 200, cy: 92}
    }, 4000);

    var c = paper.rect(0, 0, 600, 600);
    c.attr({fill: "#FFF", stroke: "#fff", opacity: 0.85})

