    twelve_foot.attr("fill", "#00F");
    twelve_foot.attr("stroke", "#000");

    var eight_foot = paper.circle(100, 100, 200);
    eight_foot.attr("fill", "#FFF");
    eight_foot.attr("stroke", "#000");

